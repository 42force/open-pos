    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "immutable": "^3.8.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
