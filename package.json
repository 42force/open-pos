    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "codelyzer": "2.0.0-beta.4",
    "electron": "1.6.2",
    "electron-connect": "",
    "electron-installer-squirrel-windows": "",
    "electron-packager": "",
    "electron-rebuild": "^1.5.7",
    "electron-squirrel-startup": "",
